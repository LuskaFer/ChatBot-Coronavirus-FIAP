    {
      "type": "response_condition",
      "output": {
        "text": {
          "values": [
            "Ainda não há medicamento específico para eliminar o coronavírus.\nVocê tem mais alguma dúvida?"
          ],
          "selection_policy": "sequential"
        }
      },
      "parent": "node_6_1586812985349",
